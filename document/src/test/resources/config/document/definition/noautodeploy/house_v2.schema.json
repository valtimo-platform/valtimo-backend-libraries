{
    "$id": "house.schema",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "House",
    "type": "object",
    "properties": {
        "street": {
            "type": "string",
            "description": "The street name.",
            "maxLength": 100
        },
        "housenumber": {
            "description": "house number must be equal to or greater than zero.",
            "type": "integer",
            "minimum": 0
        },
        "place": {
            "description": "The name of the place.",
            "type": "string",
            "maxLength": 100
        }
    }
}