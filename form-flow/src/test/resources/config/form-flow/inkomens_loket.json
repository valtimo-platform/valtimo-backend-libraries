{
    "startStep": "woonplaats",
    "steps": [
        {
            "key": "woonplaats",
            "onOpen": [
                "${'Hello '+'World!'}",
                "${4*3}",
                "${formFlowBeanTestHelper.returnTrue()}",
                "${formFlowBeanTestHelper.parseContext(step.submissionData)}"
            ],
            "onComplete": [
                "${formFlowBeanTestHelper.parseAdditionalProperties(additionalProperties)}"
            ],
            "nextSteps": [
                {
                    "condition": "${#jsonPath(step.submissionData, '$.woonplaats.inUtrecht')}",
                    "step": "leeftijd"
                },
                {
                    "condition": null,
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "leeftijd",
            "onBack": [
                "${formFlowBeanTestHelper.parseAdditionalProperties(additionalProperties)}"
            ],
            "nextSteps": [
                {
                    "condition": "${#jsonPath(step.submissionData, '$.leeftijd.isOuderDan21') || #jsonPath(step.submissionData, '$.leeftijd.isJongerDanAOW')}",
                    "step": "gezinssituatie"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "gezinssituatie",
            "nextSteps": [
                {
                    "step": "thuiswonende-kinderen"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "thuiswonende-kinderen",
            "nextSteps": [
                {
                    "step": "inkomen"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "inkomen",
            "nextSteps": [
                {
                    "condition": "${#jsonPath(step.submissionData, '$.inkomen.value') <= #jsonPath(step.submissionData, '$.gezinssituatie.vermogenGrens')}",
                    "step": "vermogen"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "vermogen",
            "nextSteps": [
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "end",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        }
    ]
}
