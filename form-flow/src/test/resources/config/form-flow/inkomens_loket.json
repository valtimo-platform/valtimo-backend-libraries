{
    "startStep": "woonplaats",
    "steps": [
        {
            "key": "woonplaats",
            "onOpen": ["${'Hello '+'World!'}",
                "${4*3}",
                "${formFlowBeanTestHelper.returnTrue()}",
                "${formFlowBeanTestHelper.parseContext(step.submissionData)}"],
            "onComplete": [
                "${formFlowBeanTestHelper.parseAdditionalProperties(additionalProperties)}"
            ],
            "nextSteps": [
                {
                    "condition": "${step.submissionData.woonplaats.inUtrecht == true}",
                    "step": "leeftijd"
                },
                {
                    "condition": null,
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "leeftijd",
            "nextSteps": [
                {
                    "condition": "${submissionData.leeftijd.isOuderDan21 == true && submissionData.leeftijd.isJongerDanAOW == true}",
                    "step": "gezinssituatie"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "gezinssituatie",
            "nextStep": "thuiswonende-kinderen",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "thuiswonende-kinderen",
            "nextStep": "inkomen",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "inkomen",
            "nextSteps": [
                {
                    "condition": "${submissionData.inkomen.value <= submissionData.gezinssituatie.vermogenGrens}",
                    "step": "vermogen"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "vermogen",
            "nextStep": "end",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "end",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        }
    ]
}