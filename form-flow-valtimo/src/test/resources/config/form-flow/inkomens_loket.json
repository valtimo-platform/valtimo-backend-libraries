{
    "startStep": "woonplaats",
    "steps": [
        {
            "key": "woonplaats",
            "nextSteps": [
                {
                    "condition": "${step.submissionData.woonplaats.inUtrecht == true}",
                    "step": "leeftijd"
                },
                {
                    "condition": null,
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "leeftijd",
            "nextSteps": [
                {
                    "condition": "${step.submissionData.leeftijd.isOuderDan21 == true && step.submissionData.leeftijd.isJongerDanAOW == true}",
                    "step": "gezinssituatie"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "gezinssituatie",
            "nextSteps": [
                {
                    "step": "thuiswonende-kinderen"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "thuiswonende-kinderen",
            "nextSteps": [
                {
                    "step": "inkomen"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "inkomen",
            "nextSteps": [
                {
                    "condition": "${step.submissionData.inkomen.value <= step.submissionData.gezinssituatie.vermogenGrens}",
                    "step": "vermogen"
                },
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },

        {
            "key": "vermogen",
            "nextSteps": [
                {
                    "step": "end"
                }
            ],
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        },
        {
            "key": "end",
            "type": {
                "name": "form",
                "properties": {
                    "definition": "my-form-definition"
                }
            }
        }
    ]
}